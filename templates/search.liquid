{% if search.performed %}
    <h1>Here Are Your Results</h1>
    {% for result in search.results %}
      <div>
        {% if result.object_type == "page" %}
          <a href="{{ result.url }}">
            <h2>{{ result.title }}</h2>
          </a>
        {% elsif result.object_type == "article" %}
          <a href="{{ result.url }}">
            <h2>{{ result.title }}</h2>
          </a>
        {% elsif result.object_type == "product" %}
          <a href="{{ result.url }}">
            <h2>{{ result.title }}</h2>
          </a>
        {% else %}
          <a href="{{ result.url }}">
            <h2>{{ result.title }}</h2>
          </a>
        {% endif %}
      </div>
    {% endfor %}
  {% else %}
    <h3>Try searching for something</h3>
  {% endif %}
  